

<!DOCTYPE html>
<html>
<head>
 
    <base href="~/" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" rel="stylesheet">
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Mshop.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.carousel.min.css">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.3.4/assets/owl.theme.default.min.css">

    <link rel="stylesheet" href="~/css/bs5mod/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/bootstrap-icons.css" />

    <link rel="stylesheet" type="text/css" href="//cdn.jsdelivr.net/bootstrap/3/css/bootstrap.css" />
    <link rel="stylesheet" href="~/css/bs5mod/bootstrap.min.css" />

    <link rel="stylesheet" href="~/css/bootstrap-icons.css" />
    <link rel="stylesheet" href="~/css/advertisement.css" />
    <script src="~/js/jquery-3.6.0.min.js"></script>
    <script src="~/js/advertisement.js"></script>
    <script src="~/js/bs5/bootstrap.bundle.min.js"></script>
    <script type="text/javascript" src="~/js/jquery.form.js"></script>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons/font/bootstrap-icons.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css" rel="stylesheet">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/js/bootstrap.bundle.min.js"></script>

</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="col-lg-3 col-md-4 col-sm-6 col-12 navbar-brand" asp-controller="Home" style="letter-spacing: 0.07em; font: caption;" asp-action="MainPage">~monkey shop~</a>
                <a class="col-lg-3 col-md-4 col-sm-6 col-12 navbar-brand" asp-controller="Home" style="letter-spacing: 0.07em; font: caption;" asp-action="ShopPage">Магазин</a>
                <a class="heart-layout navbar-brand bi bi-heart-fill" asp-controller="Home" style="letter-spacing: 0.07em; font: caption;" asp-action="FavoritesPage" />
                <a class="heart-layout navbar-brand bi bi-cart" asp-controller="Cart" style="letter-spacing: 0.07em; font: caption;" asp-action="CartPage" />
                <a class="navbar-brand" asp-controller="Advertisement" style="letter-spacing: 0.07em; font: caption;" asp-action="AdvertisementView">Создать объявление</a>
                <a class="col-lg-3 col-md-4 col-sm-6 col-12 navbar-brand" asp-controller="Home" style="letter-spacing: 0.07em; font: caption;" asp-action="Logout">Выйти</a>
            </div>
        </nav>
    </header>

    <div class="container">
        <main role="main" class="pb-3">
            @if (IsSectionDefined("Attic"))
            {
                @RenderSection("Attic", false)
            }
            @RenderBody()
        </main>
    </div>
    <script src="_framework/blazor.server.js"></script>
    <script src="~/js/common/blazor.goon.js"></script>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
